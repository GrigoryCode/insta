<div class="col-md-6 col-md-offset-3">
  <% @pics.each do |pic| %>
    <div class="panel panel-default">
      <div class="panel-body">
        <%= link_to (image_tag pic.image.url, class: 'pic-image'), pic %>
        <h3>
          <%= link_to pic.title, pic %>
        </h3>
      </div>
    </div>
  <% end %>
</div>
